pipeline {
    agent any{}
    tools{
        maven 'maven'
    }
    stages {
        stage("Checkout") {
            steps {
                git : 'https://github.com/Pranjal6955/TEST2.git' , branch: 'main'
            }
        }
        stage("Build") {
            steps {
                sh 'mvn clean package'
            }
        }
        stage("Test") {
            steps {
                sh 'mvn test'
            }
        }
        stage("Deploy") {
            steps {
                echo 'Deploying the project'
            }
        }
    //     stage('Run JAR'){
    //         steps{
    //             def output=sh(script: 'java -jar target/jenkins-test-0.0.1-SNAPSHOT.jar', returnStdout: true)
    //             echo "Output: ${output}"
    //     }
    // }
}
}